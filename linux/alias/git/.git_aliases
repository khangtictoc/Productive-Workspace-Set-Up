# AIO Commit
function gacp {
  COMMIT_MESSAGE=$1
  git add .; git commit -m "$COMMIT_MESSAGE"; git push
}

function gacpn {
  COMMIT_MESSAGE=$1
  git add .; git commit -m "$COMMIT_MESSAGE"; git push --no-verify
}

