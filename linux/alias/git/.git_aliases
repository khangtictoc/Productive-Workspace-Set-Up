}

function git-swprofile {
  PROFILE_NAME=$1
  curl -sL "https://raw.githubusercontent.com/khangtictoc/Productive-Workspace-Set-Up/refs/heads/main/linux/utility/configuration/git/profile/$PROFILE_NAME.sh" | bash 
  echo "You have switched to the $PROFILE_NAME profile!"
}

function git-ckprofile {
  git config --list | grep "user.email="
}

# Remove outdated branches
function  git-rmob {
  echo "Removing outdated branches..."
  for branch in $(git branch --format='%(refname:short)'); do
    if ! git show-ref --quiet refs/remotes/origin/$branch; then
      echo "Local branch '$branch' has no matching remote. Consider deleting it with: git branch -d $branch"
    fi
  done
}




