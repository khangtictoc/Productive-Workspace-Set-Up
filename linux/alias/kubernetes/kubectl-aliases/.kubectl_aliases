###### General
alias kgall='kubectl get all'
alias kgallall='kubectl get all --all-namespaces'
alias kcp='kubectl cp'
alias ke='kubectl events'
alias kgcs='kubectl get componentstatuses'


## Context
alias kc='kubectl config'
alias kcc='kubectl config current-context'
alias kcu='kubectl config use-context'
alias kcg='kubectl config get-contexts'
alias kcrnc='kubectl config rename-context'
alias kcv='kubectl config view'
alias kcvf='kubectl config view --flatten'
alias kcs='kubectl config set-context'
alias kcscn='kubectl config set-context $(kubectl config current-context) --namespace'
alias kcsn='kubectl config set-context $(kubectl config current-context) --namespace'
alias kcrmcl='kubectl config delete-cluster'   
alias kcrmc='kubectl config delete-context'  
alias kcrmu='kubectl config delete-user'
# List contexts with more details  
alias kcgd='wget -qO- https://raw.githubusercontent.com/khangtictoc/Productive-Workspace-Set-Up/refs/heads/main/linux/alias/alias_script/kubernetes/get_details_context.py | python3 -'
# List nodes with requests/limits 
alias kgnorl='wget -qO- https://raw.githubusercontent.com/khangtictoc/Productive-Workspace-Set-Up/refs/heads/main/linux/alias/alias_script/kubernetes/node__get_resource_req_lim.py | python3 -'
# List ingresses with paths
alias kgingpth='kubectl get ingress -o custom-columns="NAME:.metadata.name,DOMAIN:.spec.rules[*].host,PATHS:.spec.rules[*].http.paths[*].path"'

## Apply configuration
alias ka='kubectl apply -f'
alias kar='kubectl apply --recursive -f'


## Node
alias ktn='kubectl top nodes'

###### Dedicated Functions ######

## Node
# Get nodes with labels
kgnos(){
    SELECTOR_LABEL=$1
    kubectl get nodes --selector="$SELECTOR_LABEL"
}

kcrmcfg(){
    CONTEXT_NAME=$1
    kubectl config delete-cluster $CONTEXT_NAME
    kubectl config delete-context $CONTEXT_NAME
    kubectl config delete-user $CONTEXT_NAME
}

kcvthisns(){
    CURRENT_CONTEXT=$(kubectl config current-context)
    kubectl config view -o jsonpath="{$.contexts[?(@.name=='$CURRENT_CONTEXT')].context.namespace}"
}
kgpobyno(){
    NODE_NAME=$1
    kubectl get pods -o wide --all-namespaces --field-selector spec.nodeName=$NODE_NAME
}

kpa(){
    RESOURCE_TYPE=$1
    RESOURCE_NAME=$2
    kubectl patch -p '{"metadata":{"finalizers":null}}' --type=merge $RESOURCE_TYPE $RESOURCE_NAME
}


